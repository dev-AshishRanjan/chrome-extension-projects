{
    "name": "Language Recognizer",
    "version": "1.0.1",
    "description": "A chrome extension inspired for language detection of your website",
    "icons": {
        "128": "icon_128.png"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "lang.html"
    },
    "permissions": [
        "activeTab"
    ],
    "manifest_version": 2
}